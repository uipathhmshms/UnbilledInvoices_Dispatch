                       If(paymentCondition.Contains("יום"), 
                          Convert.ToInt32(System.Text.RegularExpressions.Regex.Match(paymentCondition, "\d+").Value), 
                          0)) ' Handle "X יום" or fallback to 0
    Let calculatedDate =endOfMonth.AddDays(daysToAdd)' Add days only if endOfMonth is valid
    Let valueDate = If(row.IsNull("תאריך ערך"), Nothing, row.Field(Of DateTime)("תאריך ערך")) ' Properly use "תאריך ערך"
Where calculatedDate &lt;= valueDate AndAlso row("סטטוס").ToString="נרשם" ' Check valid dates and comparison
Select row).CopyToDataTable]</InArgument>
